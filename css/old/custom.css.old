@font-face {
    font-family: 'ostrich_sansmedium';
    src: url('../fonts/ostrich-regular-webfont.eot');
    src: url('../fonts/ostrich-regular-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/ostrich-regular-webfont.woff') format('woff'),
         url('../fonts/ostrich-regular-webfont.ttf') format('truetype'),
         url('../fonts/ostrich-regular-webfont.svg#ostrich_sansmedium') format('svg');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'byngve_lt_stdregular';
    src: url('../fonts/byngve-lt-std_8890-webfont.eot');
    src: url('../fonts/byngve-lt-std_8890-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/byngve-lt-std_8890-webfont.woff') format('woff'),
         url('../fonts/byngve-lt-std_8890-webfont.ttf') format('truetype'),
         url('../fonts/byngve-lt-std_8890-webfont.svg#byngve_lt_stdregular') format('svg');
    font-weight: normal;
    font-style: normal;

}

.font_ostrich
{
	/*font-family: 'ostrich_sansmedium';*/
    /*font-family: 'hero_lightlight';*/
    font-family: 'code_lightregular';
    font-weight: 600;
	-webkit-text-size-adjust: 100%;
      -ms-text-size-adjust: 100%;
}

h1.font_ostrich{ font-size: (@font-size-base * 2.75); font-size: 4rem; letter-spacing: 1px;}
h2.font_ostrich{ font-size: (@font-size-base * 2.25); font-size: 3rem; letter-spacing: 1px;}
h3.font_ostrich{ font-size: (@font-size-base * 1.75); font-size: 2.5rem; letter-spacing: 1px;}
h4.font_ostrich{ font-size: (@font-size-base * 1.25); font-size: 2rem; letter-spacing: 1px;}
h5.font_ostrich{ font-size: @font-size-base; font-size: 1.6rem; letter-spacing: 1px;}
h6.font_ostrich{ font-size: (@font-size-base * 0.85); font-size: 1.2rem; letter-spacing: 1px;}

.font_huge_ostrich{ 
	font-size: (@font-size-base * 6.5); 
	font-size: 8rem;
	font-family: 'ostrich_sansmedium';
	letter-spacing: 1px;
	}

.font_big_ostrich{ 
	font-size: (@font-size-base * 2.75); 
	font-size: 4.5rem;
	font-family: 'ostrich_sansmedium';
	letter-spacing: 1px;
	}
	
.font_reg_ostrich{ 
	font-size: (@font-size-base * 2.25); 
	font-size: 3rem; 
	font-family: 'ostrich_sansmedium';
	letter-spacing: 1px;
	}
	
.font_small_ostrich{ 
	font-size: (@font-size-base * 1.75); 
	font-size: 2rem; 
	font-family: 'ostrich_sansmedium';
	letter-spacing: 1px;
	}

.font_tiny_ostrich{ 
	font-size: (@font-size-base * 1.75); 
	font-size: 1.5rem; 
	font-family: 'ostrich_sansmedium';
	letter-spacing: 1px;
	}

.font_byngve
{
	font-family: 'byngve_lt_stdregular';
	font-weight: 400;
	-webkit-text-size-adjust: 100%;
	-ms-text-size-adjust: 100%;
}


h1.font_byngve{ font-size: (@font-size-base * 2.75); font-size: 4rem; }
h2.font_byngve{ font-size: (@font-size-base * 2.25); font-size: 3rem; }
h3.font_byngve{ font-size: (@font-size-base * 1.75); font-size: 2.1rem; }
