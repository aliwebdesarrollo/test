h5.font_byngve{ font-size: @font-size-base; font-size: 1.5rem; }
h6.font_byngve{ font-size: (@font-size-base * 0.85); font-size: 1.2rem; }


.font_huge_byngve{ 
	font-size: (@font-size-base * 2.75); 
	font-size: 4rem;
