body
{
	background: url(../images/light/bg.jpg) repeat #FFF;
	padding-top: 60px;   
}

h1,h2,h3,h4,h5,h6,p,a
{
	word-break:keep-all ;
	word-wrap: break-word;
	text-align: justify;
	-moz-hyphens: auto;
	-ms-hyphens:auto;
	-o-hyphens:auto;
	-webkit-hyphens:auto;
	hyphens: auto;
}

a, a:focus, a:hover, a:active
{
	color: rgba(252,20,102,1);
	text-decoration: none;
}

hr{
	border-color: rgb(200,200,200);
}

hr.single_separator
{
	margin:5px auto 2px;
	width: 100%;
	border-color: rgb(200,200,200);
	clear: both;
}

.hr {
  background-color: #222;
  height: 1px;
  margin: 18px auto;
  width: 33.3%;
}

.no-hyphens
{
	-moz-hyphens: none;
	-ms-hyphens:none;
	-o-hyphens:none;
	-webkit-hyphens:none;
	hyphens: none;

}

#post-title
{
	margin-top: 0px;
}

.label-tags
{
	background-color: rgba(255,64,128,1);
}

.label-tags-single
{
	font-size: 60%;
}

.video-thumbnail
{
	width: 100%;
	min-height: 250px;
}

.img-full-width
{
	width: 100%;
}

.thumbnail
{
	padding: 0px;
}

.prev-thumb
{
	padding:4px;
}

footer, #footer
{
	margin-top: 20px;
}

.panel-footer
{
	background-color: #FF8080;
}

.thumb-hold
{
	opacity: 0.5;
}

.thumb-hold:active, .thumb-hold:hover
{
	opacity: 1;
}

.form-custom
{
	padding:10px;
}

.panel-social a
{
	margin:0px 10px;
}

.panel-social a span
{
	font-size: 36px;
	color: #FF8080;
	padding:0px 5px;
	opacity: 0.6;
}

.panel-social a span:hover
{
	opacity: 1;
}

.menu-item-home
{
	font-family: "ostrich_sansmedium";
	font-size: 1.9rem;
}


.adverts-art 
{
	border-radius:20px;
}

.Tags-Style 
{
	padding: 2px 5px;
	display: block;
}

.Tags-Style label
{
	display: inline-block;
	transform: skew(-15deg);
	-webkit-transform: skew(-15deg);
	-moz-transform: skew(-15deg);
	-o-transform: skew(-15deg);
	-ms-transform: skew(-15deg);
	background-color: #ff6060;
	padding: 3px 5px 1px;
	margin:0px 0px 0px 5px;
	opacity: 0.8;
}

.Tags-Style label:nth-child(2n)
{	
	background-color: #fff;
}

.Tags-Style label:hover
{
	opacity: 1;
}

.Tags-Style label p
{
	transform: skew(15deg);
	-webkit-transform: skew(15deg);
	-moz-transform: skew(15deg);
	-o-transform: skew(15deg);
	-ms-transform: skew(15deg);
	margin: 0px;
	font-family: 'byngve_lt_stdregular';
}

.Tags-Style label p a
{
	color: #fff;
	padding: 0;
}

.Tags-Style label:nth-child(2n) p a
{
	color: #ff6060;
} 

.category-now, .tag-now
{
	color: #3E667B;
}

.panel-history-tree
{
	border-radius:0px;
	background-color: #fff;
	padding:1px 2px;
	margin-bottom: 10px;
}

.tags-moreused
{
	display: inline-block;
}

.tags-moreused a
{
	color: rgb(168, 20, 170);
	display: block;
	margin-right: 15px;
	-moz-hyphens: none;
	-ms-hyphens:none;
	-o-hyphens:none;
	-webkit-hyphens:none;
	hyphens: none;
}

.tags-moreused a:hover
{
	color: rgb(252, 20, 102);
}

.post-most-view
{
	color: #111;
}

.youtube-responsive
{
	width: 100%;
	min-height: 300px;
	margin: 20px 0px;
}

.fb-comments { width: 100%; /* or width: 50%; */ }

.fb-comments span{ width: 100% !important; /* or width: 50%; */ }

.fb-comments iframe{ width: 100% !important; /* or width: 50%; */ }

/* Large desktop */
@media (min-width: 1200px) {
	.fb-comments { width: 100%; /* or width: 50%; */ }
	.fb-comments span{ width: 100% !important; /* or width: 50%; */ }
	.fb-comments iframe{ width: 100% !important; /* or width: 50%; */ }
   .youtube-responsive{
		width: 100% !important;
		min-height: 400px !important;
		margin: 20px 0px !important;
	} 
}

/* Portrait tablet to landscape and desktop */
@media (min-width: 768px) and (max-width: 979px) {
	.fb-comments { width: 100%; /* or width: 50%; */ }
	.fb-comments span{ width: 100% !important; /* or width: 50%; */ }
	.fb-comments iframe{ width: 100% !important; /* or width: 50%; */ }
	.youtube-responsive{
		width: 100% !important;
		min-height: 350px !important;
		margin: 20px 0px !important;
	}
}

/* Landscape phone to portrait tablet */
@media (max-width: 767px) {
    .fb-comments { width: 100%; /* or width: 50%; */ }
    .fb-comments span{ width: 100% !important; /* or width: 50%; */ }
    .fb-comments iframe{ width: 100% !important; /* or width: 50%; */ }
	.youtube-responsive{
		width: 100% !important;
		min-height: 300px !important;
		margin: 15px 0px !important;
	}
}

/* Landscape phones and down */
@media (max-width: 480px) {
    .fb-comments { width: 100%; /* or width: 50%; */ }
    .fb-comments span{ width: 100% !important; /* or width: 50%; */ }
    .fb-comments iframe{ width: 100% !important; /* or width: 50%; */ }
	.youtube-responsive{
		width: 100% !important;
		min-height: 200px !important;
		margin: 10px 0px !important;
	}
}